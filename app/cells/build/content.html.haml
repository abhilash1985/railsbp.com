#build
  - if @build.try(:completed?)
    %dl.summary.clearfix
      .left
        %dt Build
        %dd #{@build.position}
        %dt Commit
        %dd #{@build.short_commit_id}
      .right
        %dt Duration
        %dd #{@build.duration} secs
        %dt Finished
        %dd #{time_ago_in_words(@build.updated_at)} ago
      %dt Message
      %dd.message #{@build.last_commit_message}
    %h3 Analyze Result
    %p Warning count: #{@build.warning_count}
    = link_to "Customize Checks", "#", :id => "customizeChecks", :class => "trigger"
    %ul#checksPanel.hidden
    = render :inline => File.read(@build.analyze_file)
